		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"chromatic": "chromatic"
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
