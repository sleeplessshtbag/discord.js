		"format": "prettier --write . && cross-env TIMING=1 eslint --fix --format=pretty src",
		"prepack": "yarn build && yarn lint",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/ui/*'",
